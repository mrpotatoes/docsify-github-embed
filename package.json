{
  "name": "docsify-github-embed",
  "version": "1.0.4",
  "description": "Embed your github code in your docsify site! Even line numbers and such!",
  "author": "Andric LibreSinn",
  "license": "MIT",
  "homepage": "https://github.com/mrpotatoes/docsify-github-embed#readme",
  "main": "src",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:start": "rollup -c -w -m",
    "dev:serve": "serve dist",
    "dev:docs": "docsify serve docs",
    "dev:open": "open-cli http://localhost:3000",
    "clean": "rimraf ./dist",
    "build": "rollup -c"
  },
  "keywords": [
    "docsify",
    "github",
    "embed"
  ],
  "files": [
    "src",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrpotatoes/docsify-github-embed.git"
  },
  "dependencies": {
    "open-cli": "^5.0.0",
    "serve": "^11.2.0",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.1.6",
    "@babel/runtime": "^7.7.6",
    "@rollup/plugin-node-resolve": "6.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.2",
    "rollup": "1.27.10",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-postcss": "2.0.3",
    "rollup-plugin-uglify": "^6.0.4",
    "uglify-js": "3.7.2"
  }
}
