{
  "name": "docsify-github-embed",
  "version": "1.1.2",
  "description": "sidebar-collapse plugin for docsify",
  "main": "src",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:start": "rollup -c -w",
    "dev:serve": "serve dist",
    "dev:docs": "docsify serve docs",
    "deb:open": "open-cli http://localhost:3000",
    "clean": "rimraf ./dist",
    "build": "run-s build:*",
    "build:rollup": "rollup -c",
    "build:minify": "uglifyjs --compress --mangle --output ./dist/docsify-sidebar-collapse.min.js -- ./dist/docsify-sidebar-collapse.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iPeng6/docsify-sidebar-collapse.git"
  },
  "keywords": [
    "docsify",
    "sidebar",
    "collapse"
  ],
  "files": [
    "src",
    "dist"
  ],
  "author": "ipeng6",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iPeng6/docsify-sidebar-collapse/issues"
  },
  "homepage": "https://github.com/iPeng6/docsify-sidebar-collapse#readme",
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.1.6",
    "@babel/runtime": "^7.7.6",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.2",
    "rollup": "1.27.10",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.1.0",
    "@rollup/plugin-node-resolve": "6.0.0",
    "rollup-plugin-postcss": "2.0.3",
    "uglify-js": "3.7.2"
  },
  "dependencies": {
    "open-cli": "^5.0.0",
    "serve": "^11.2.0",
    "whatwg-fetch": "^3.0.0"
  }
}
